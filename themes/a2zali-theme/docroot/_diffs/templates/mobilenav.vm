<nav class="pushmenu pushmenu-left" id="navigation" role="navigation">
	<div id="pushMenuclose"><i class="fa fa-close"></i></div>
	<div class="container-fluid">
		<ul aria-label="#language ("site-pages")" class="links" role="menubar">
			#foreach ($nav_item in $nav_items)
				<li class="$nav_item_css_class nav_li" id="layout_$nav_item.getLayoutId()" $nav_item_attr_selected role="presentation">
					<a aria-labelledby="layout_$nav_item.getLayoutId()" $nav_item_attr_has_popup class="$nav_item_link_css_class" href="$nav_item.getURL()" $nav_item.getTarget() role="menuitem">
						<span>$nav_item.icon() $nav_item.getName() $nav_item_caret</span>
					</a>
					#if ($nav_item.hasChildren())
						<ul class="dropdown-menu child-menu" role="menu">
							#foreach ($nav_child in $nav_item.getChildren())
								<li class="$nav_child_css_class" id="layout_$nav_child.getLayoutId()" $nav_child_attr_selected role="presentation">
									<a aria-labelledby="layout_$nav_child.getLayoutId()" href="$nav_child.getURL()" $nav_child.getTarget() role="menuitem">$nav_child.getName()</a>
									#if ($nav_child.hasChildren())
										<ul class="grandchildmenu" role="menu">
											#foreach ($nav_grandchild in $nav_child.getChildren())
												<li class="$nav_grandchild_css_class" id="layout_$nav_grandchild.getLayoutId()" $nav_grandchild_attr_selected role="presentation">
													<a aria labelledby ="layout_$nav_grandchild.getLayoutId()" href ="$nav_grandchild.getURL()" $nav_grandchild.getTarget() role=" menuitem ">
														<span>$nav_grandchild.icon() $nav_grandchild.getName() </span>
													</a>
												</li>
											#end
										</ul>
									#end
								</li>
							#end
						</ul>
					#end
				</li>
			#end
		</ul>
		#if ($layout.isPrivateLayout())
			<ul class="links pull-right"">
				<li class="backhome">
					<a href="$company_url"> &#10149; Back to Dashboard </a>				
				</li>
			</ul>
		#end
	</div>
</nav>
<div id="nav_list"><i aria-hidden="true" class="fa fa-bars"></i></div>